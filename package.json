{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve:ssr:frontend": "node dist/frontend/server/server.mjs",
    "dev:ssr": "ng run frontend:serve-ssr",
    "dev:ssr:prod": "ng run frontend:serve-ssr:production",
    "staging:ssr": "ng run frontend:serve-ssr:staging",
    "serve:ssr": "node dist/frontend/server/main.js",
    "prerender": "ng run frontend:prerender:production --routes-file routes.txt",
    "prerender:dev": "ng run frontend:prerender:development --routes-file routes.txt",
    "prerender:staging": "ng run frontend:prerender:staging --routes-file routes.txt",
    "build:prod:ssr": "ng build --configuration production --aot --vendor-chunk --common-chunk --delete-output-path --build-optimizer --output-hashing=none && ng run frontend:server:production",
    "build:staging:ssr": "ng build --configuration staging --aot --vendor-chunk --common-chunk --delete-output-path --build-optimizer --output-hashing=none && ng run frontend:server:staging",
    "build:ssr": "ng build --aot --vendor-chunk --common-chunk --delete-output-path --build-optimizer --output-hashing=none && ng run frontend:server"
  },
  "private": true,
  "dependencies": {
